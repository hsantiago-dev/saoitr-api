generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Contributor {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String
  occurrences   Occurrence[]
}

model Occurrence {
  id Int @id @default(autoincrement())
  date String
  time String
  local String
  type String
  km String
  contributor   Contributor @relation(fields: [contributorId], references: [id])
  contributorId Int
}